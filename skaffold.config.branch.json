{
    "name": "hyperion-history-api",
    "services": [
      {
        "name": "hyperion-history-api",
        "chartPath": "helm/",
        "upgradeOnChange": true,
        "createNamespace": true,
        "valuesFiles": ["helm/values/local.yaml"],
        "artifactOverrides": {
          "deployments": {
            "voice-eos-hyperion-api" : {
              "containers": {
                "voice-eos-hyperion-api": {
                  "image": "gcr.io/voice-dev-infra-services/voice/voice-eos-hyperion"
                }
              }
            }
          }
        }
      }
    ],
    "helm": {
      "flags": {
        "install": [
          "--timeout=1200s"
        ],
        "upgrade": [
          "--timeout=1200s"
        ]
      }
    }
  }
  